


create Table Accountant(
    -> accountantID int PRIMARY KEY AUTO_INCREMENT,
    -> name varchar(20) not null,
    -> username varchar(20) not null,
    -> password varchar(20) not null,
    -> email varchar(20) unique not null);
    
